<h1>New Contact</h1>

<%= form_with model: @contact do |form| %>
  <p>
    <%= form.label "first_name", "Contact First Name" %>
    <%= form.text_field "first_name" %>

    <%= form.label "last_name", "Contact Last Name" %>
    <%= form.text_field "last_name" %>
  </p>

  <p>
    <%= form.hidden_field "company_id" %>
  </p>
  <%= form.submit %>
<% end %>